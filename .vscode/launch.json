{
  "configurations": [
      {
          "name": "Cortex Debug (STM32F767 ST-Link)",   // Required VSCode extension: "Cortex-Debug"
          "cwd": "${workspaceRoot}",
          "executable": "${workspaceRoot}/build/src/armv7m/YesRTOSMachineLayer.elf",
          "request": "launch",
          "type": "cortex-debug",
          "servertype": "openocd",
          "configFiles": [
              "interface/stlink.cfg",
              "target/stm32f7x.cfg",
          ],
          "openOCDLaunchCommands": ["init", "reset init"],
          "runToEntryPoint": "main",    // Debug symbol to place first breakpoint on.
          "svdFile": "${workspaceRoot}/armv7m/STM32F767.svd"
      }
  ]
}